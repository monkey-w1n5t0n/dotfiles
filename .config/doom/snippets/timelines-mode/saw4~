  "amp" <>< let
    in vol $ ${2:0}
  "freq1" <>< let
    in ${3:f}
  "freq2" <>< let
    in ${4:f}
  "freq3" <>< let
    in ${5:f}
  "freq4" <>< let
    in ${6:f}
  "width" <>< let
    in ${7:0.5}
  "pan" <>< let
    in ${0:0}